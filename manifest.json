{
  "manifest_version": 3,
  "name": "TabSphere",
  "version": "1.0.0",
  "description": "Simple new tab override.",
  "chrome_url_overrides": {
    "newtab": "./index.html"
  },
  "icons": {
    "16": "./icons/icon16.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "permissions": [
    "storage"
  ]
}
